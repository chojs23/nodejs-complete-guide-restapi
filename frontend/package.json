{
  "name": "node-complete-social-network-prep",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "eslint": "^8.28.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^5.0.1"
  },
  "scripts": {
    "start": "ESLINT_NO_DEV_ERRORS='true'  react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-vars": "warn"
    },
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "max-len": "warn"
        }
      }
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
